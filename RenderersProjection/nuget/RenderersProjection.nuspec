<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>RenderersWRC</id>
    <version>0.1.0-prerelease</version>
    <authors>Volker Veidt</authors>
    <description>A simple component with basic math operations and SwapChainPanel for renderer</description>
    <dependencies>
      <!--Note: Target .NET 5 by replacing instances of "net6.0" with "net5.0 in this file"-->
      <group targetFramework="net6.0-windows10.0.22621.0" />
    </dependencies>
  </metadata>
  <files>
    <!--Note: This sample assumes the solution configuration and platform are set to Release and x64-->
    <file src="..\..\_build\AnyCPU\Release\RenderersProjection\bin\RenderersProjection.dll" target="lib\net6.0-windows10.0.22621.0\RenderersProjection.dll" />
    <!--Arch-specific implementation DLLs should be copied into RID-relative folders-->
    <file src="..\..\_build\x64\Release\RenderersWRC\bin\RenderersWRC\RenderersWRC.dll" target="runtimes\win10-x64\native\RenderersWRC.dll" />
    <!--To support x86 and ARM64, build RenderersWRC for those other architectures and uncomment the entries below.-->
    <!--<file src="..\..\_build\Win32\Release\RenderersWRC\bin\RenderersWRC\RenderersWRC.dll" target="runtimes\win10-x86\native\RenderersWRC.dll" />-->
    <!--<file src="..\..\_build\arm64\Release\RenderersWRC\bin\RenderersWRC\RenderersWRC.dll" target="runtimes\win10-arm64\native\RenderersWRC.dll" />-->
  </files>
</package>